{
  "name": "Open in specific window",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "Opens a link in a specified window",
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Name this window",
    "default_popup": "browserAction.html"
  }
}
